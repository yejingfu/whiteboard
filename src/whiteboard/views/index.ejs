<% include header.ejs %>

<% include welcome.ejs %>

<!-- main -->
<div class="container" role="main">
  <row>
    <div class="col-sm-8">
      <div class="panel panel-default" style="margin-top:4px;">
        <div class="panel-body" style="padding:2px;">
          <div id="canvas-container" style="background-color:rgb(248, 248, 248)">
            <canvas id="canvas-main" width="720" height="600" data-paper-scope="1" style="-webkit-user-select: none; -webkit-user-drag: none; -webkit-tap-highlight-color: rgba(0,0,0,0);"></canvas>
          </div>
        </div>
      </div>
    </div>
    <div class="col-sm-3 col-sm-offset-1">
      <div class="panel panel-default" style="margin-top:4px;">
        <div class="panel-body">
          <div id="video-container">
            <video id="video-local" autoplay="autoplay" width="220" height="200"></video>
          </div>
        </div>
      </div>
    </div>
  </row>
</div>

